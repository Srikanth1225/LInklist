#include <stdio.h>
#include <stdlib.h>

struct node {
    int data;
    struct node *next;
};

typedef struct node nd;

void searchList(nd *head, int key) {
    nd *temp = head;
    int position = 1;
    
    while (temp != NULL) {
        if (temp->data == key) {
            printf("Element %d found at position %d\n", key, position);
            return;
        }
        temp = temp->next;
        position++;
    }
    
    printf("Element %d not found in the list.\n", key);
}
